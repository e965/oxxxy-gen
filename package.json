{
    "name": "oxxxy-gen",
    "description": "Генератор текстов Оксимирона",
    "private": true,
    "version": "1.0.0",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:e965/oxxxy-gen",
    "license": "MIT",
    "scripts": {
        "prettier": "prettier --write **/*.{html,json,js,css}",
        "lint-staged": "lint-staged",
        "deploy": "gh-pages -d src"
    },
    "lint-staged": {
        "*.{html,json,js,css}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "devDependencies": {
        "gh-pages": "^3.0.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.7",
        "prettier": "^2.0.5"
    },
    "dependencies": {}
}
